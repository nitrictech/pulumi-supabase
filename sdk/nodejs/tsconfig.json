{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "functions/dbFunction.ts",
        "functions/index.ts",
        "index.ts",
        "organizations/index.ts",
        "organizations/organization.ts",
        "projects/applyMigrations.ts",
        "projects/index.ts",
        "projects/project.ts",
        "provider.ts",
        "secrets/index.ts",
        "secrets/secret.ts",
        "storage/bucket.ts",
        "storage/index.ts",
        "utilities.ts"
    ]
}
